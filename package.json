{
    "name": "sfuapi",
    "version": "1.0.0",
    "description": "An asynchronous TypeScript wrapper for the SFU API.",
    "main": "lib/index.js",
    "types": "lib/types/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "prepare": "ts-patch install -s",
        "test": "jest",
        "build": "tsc",
        "clean": "rimraf lib",
        "generate-docs": "typedoc src/index.ts --out ./docs",
        "format:check": "prettier --list-different './**/*.ts' './**/*.json'",
        "format:fix": "prettier --write './**/*.ts' './**/*.json'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ewanbrinkman/sfuapi.git"
    },
    "author": "Ewan Brinkman",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ewanbrinkman/sfuapi/issues"
    },
    "homepage": "https://github.com/ewanbrinkman/sfuapi#readme",
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.3",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-patch": "^3.0.2",
        "typedoc": "^0.25.2",
        "typescript": "^5.2.2",
        "typescript-transform-paths": "^3.4.6"
    },
    "dependencies": {
        "axios": "^1.5.1"
    }
}
